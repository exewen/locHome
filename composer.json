{
    "name": "exeweb/lochome",
    "description": "",
    "keywords": ["lochome", "laravel"],
    "license": "",
    "authors": [
        {
            "name": "exeweb",
            "homepage": "http://loc.exeweb.cn"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "predis/predis" : "~1.0",
        "mews/captcha": "^2.1",
        "phpoffice/phpexcel":"1.8.1",
        "camroncade/timezone": "0.1",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "torann/geoip": "0.2.*@dev",
        "maatwebsite/excel": "~2.0.0",
        "simplesoftwareio/simple-qrcode": "1.3.*",
        "picqer/php-barcode-generator": "^0.2.1",
        "caffeinated/modules": "^2.0",
        "prettus/l5-repository": "^2.6",
        "aws/aws-sdk-php-laravel": "^3.1",
        "aws/aws-sdk-php": "~3.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "intervention/image": "^2.3",
        "ps/image-optimizer": "^1.0",
        "zizaco/entrust": "^1.8",
        "doctrine/inflector":"1.0",
        "rmccue/requests": ">=1.0",
        "owen-it/laravel-auditing": "v4.1.4",
        "psr/log": "1.0.2",
        "barryvdh/laravel-dompdf": "^0.8.0",
        "php-amqplib/php-amqplib":">=2.6.1",
        "jormin/laravel-baidu-speech": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "yzalis/identicon": "^1.1",
        "phpunit/phpunit": "5.5.*",
        "phpspec/phpspec": "~2.1",
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0",
        "barryvdh/laravel-ide-helper": "^2.1",
        "barryvdh/laravel-debugbar": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [

        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize",
            "php artisan ide-helper:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    }
}
